{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "bun run --hot --no-clear-screen src/index.ts",
    "start": "bun run dist/index.js",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/google": "catalog:",
    "@hono/trpc-server": "^0.4.0",
    "@sightmap/api": "workspace:*",
    "@sightmap/auth": "workspace:*",
    "@sightmap/common": "workspace:*",
    "@sightmap/db": "workspace:*",
    "@trpc/server": "catalog:",
    "ai": "catalog:",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "hono": "catalog:",
    "multer": "^2.0.2",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "^1.2.6",
    "@types/multer": "^2.0.0",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}
